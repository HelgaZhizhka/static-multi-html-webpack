{
  "name": "Webpack5 multi-page static pages",
  "version": "0.0.1",
  "description": "Multi-page static pages template with one layout and include other modules to location. HTML Sass",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "multi page",
    "static",
    "setup"
  ],
  "scripts": {
    "dev": "webpack --mode development  --config configuration/webpack.dev.config.js",
    "watch": "webpack --mode development --watch  --config configuration/webpack.dev.config.js",
    "start": "webpack serve --no-client-overlay-warnings --open --mode development --config configuration/webpack.dev.config.js",
    "production": "webpack --mode production --config configuration/webpack.prod.config.js",
    "lint:fix": "eslint --fix src/js/ --color",
    "lint:sass": "sass-lint -v -q",
    "stats": "webpack --mode production  --config configuration/webpack.prod.config.js --analyze",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-partials-plugin": "^0.8.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "image-minimizer-webpack-plugin": "^3.6.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.54.9",
    "sass-lint": "^1.13.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0"
  }
}
